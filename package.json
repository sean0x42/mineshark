{
  "name": "mineshark",
  "version": "1.0.0-indev",
  "description": "A packet sniffing proxy that sits between a Minecraft client and server.",
  "main": "dist/index.js",
  "author": "Sean Bailey <hello@seanbailey.dev>",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts | bunyan",
