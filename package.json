    "lint": "eslint \"src/**\"",
    "readNbt": "ts-node scripts/readNbtToNumberArray.ts"
  },
  "dependencies": {
    "bunyan": "^1.8.15",
    "immer": "^9.0.6"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.7",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.6.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.2",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.2.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  }
